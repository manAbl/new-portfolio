{
  "name": "new-portfolio",
  "version": "1.0.0",
  "description": "My new portfolio web-app with nuxt.js",
  "author": "Manuel Blanco",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
